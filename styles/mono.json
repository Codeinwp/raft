        "typography": {
          "fontWeight": "400"
        }
      },
      "h6": {
        "typography": {
          "fontWeight": "400"
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--spline-sans-mono)"
    }
  }
}


