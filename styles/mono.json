        {
          "slug": "raft-fg",
          "color": "#FFFFFF",
          "name": "Text"
        },
        {
          "slug": "raft-accent",
          "color": "#0B33C1",
          "name": "Accent"
        },
        {
          "slug": "raft-accent-secondary",
          "color": "#1041F1",
          "name": "Accent Secondary"
        },
        {
          "slug": "raft-bg-inv",
          "color": "#131435",
          "name": "Dark Background"
        },
        {
          "slug": "raft-bg-alt",
          "color": "#20225B",
          "name": "Light Background"
        },
        {
          "slug": "raft-fg-alt",
          "color": "#F8F8F8",
          "name": "Inverted Text"
